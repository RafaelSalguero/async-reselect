{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},"**/*.map": {"when": "$(basename).map"},
        "**/**.js": { "when": "$(basename).tsx" }
    },
    "typescript.tsdk": "node_modules\\typescript\\lib"
}