{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "inlineSourceMap": true,
        "declaration": true,
        "strictNullChecks": true,
        "lib": [
            "es6",
            "dom",
            "dom.iterable"
        ],
        "downlevelIteration": true,
        "noImplicitThis": true,
        "outDir": "dist",
        "rootDir": "src"
    }
}